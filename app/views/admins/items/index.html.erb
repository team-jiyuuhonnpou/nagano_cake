<!---Admins::Items#index--->
<div>
  <div>
    <div>
      <h5>商品一覧</h5>
      <%= link_to new_admins_item_path do %>
        <i class="btn btn-plus-circle"></i>
      <% end %>
      <table class=table>
        <thead>
          <tr>
            <th></th>
            <th>商品ID</th>
            <th>商品名</th>
            <th>税抜価格</th>
            <th>ジャンル</th>
            <th>ステータス</th>
          </tr>
        </thead>
        <% @items.each do |item| %>
        <tbody>
          <td>
            <th><%= item.id %></th>
            <th><%= item.item_name %></th>
            <th><%= item.non_taxed_price %></th>
            <th><%= item.genre.name %></th>
            <th><%= item.is_active %></th>
          </td>
        </tbody>
        <% end %>
        <%= paginate @items %>
      </table>
    </div>
  </div>
</div>
